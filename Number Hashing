 #include<bits/stdc++.h>
using  namespace std;

int main(){
    int size;
    cout<<"Ente the size of the array ";
    cin>>size;
    int arr[size];
     for(int i=0;i<size;i++){
         cout<<"Enter the element  "<<i+1<<endl;
         cin>>arr[i];
     }
    
    
    // yha main kaam chal rha hai 
    int hash[size+1]={0};
    
    for(int i=0;i<size;i++){
        hash[arr[i]]++;
    }
    
    int queries;
    cout<<"\n Enter the number of queries "<<endl;
    cin>>queries;
    while(queries--){ 
    int number;
    
    cin>>number;
    
    cout<<"Number "<< number<<" Occurs "<<hash[number]<<" times in the array ";
        
        
    }
    
    
    return 0;
}
